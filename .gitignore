# Ignore bundler config.
/.bundle
Gemfile.lock

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

# Ignore vendor
/vendor/*
/vendor
/tmp/cache/assets/sprockets/*
/tmp/cache/assets/sprockets
/log/development.log

# Ignore
/config/database.yml
/config/secrets.yml
.byebug_history
db/schema.rb
